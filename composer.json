{
    "name": "aaronbullard/paper-trail",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Bullard",
            "email": "aaron.bullard77@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "mikemccabe/json-patch-php": "^0.1.0",
        "aaronbullard/php-schema": "dev-master",
        "swaggest/json-diff": "^3.5",
        "mockery/mockery": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "PaperTrail\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "files": ["tests/helpers.php"]
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always --coverage-html=./coverage --coverage-text=tests/summary.txt"
    }
}
